generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EscapeResult {
  id        Int      @id @default(autoincrement())
  result    String   // "WIN" or "FAIL"
  timeTaken Int?     // null if failed
  createdAt DateTime @default(now())
}
